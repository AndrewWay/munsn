<div class="commTemp" id="{{_id}}">
	<div class="commInfo">
		<div class="commPic">
			<img src="/content/image/profile/{{uid}}">
		</div>
		<div class="commName">
			<a href="/profile/{{uid}}"> {{fname}} {{lname}} </a> <br>
			<span>{{date}}</span>
		</div>
	</div>
	<div class="commCont">
			<p> {{text}} </p>
	</div>
</div>