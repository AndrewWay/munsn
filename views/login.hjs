<html>

<head>
	<title> {{ title }} </title>
	<link rel="stylesheet" type="text/css" href="/stylesheets/login.css">
	
	<meta content="text/html;charset=utf-8" http-equiv="Content-Type">
	<meta content="utf-8" http-equiv="encoding">
	

	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
	<script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
	<link rel="stylesheet" href="http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css" />

	<script src="/js/login.js"></script>
</head>

<body onload="document.loginForm.reset(); document.regForm.reset();">
	<div id="sideBar">
			
	</div>
		

	<div id="wrapper">
	<div id="obscure">
	</div>
		<div id="leftPanel">

			<div id="loginPanel">
				<form id="loginFields" name="loginForm" method="post">
					<input class="logField" type="text" name="user" placeholder="Username"><br>
					<input class="logField" type="password" name="pass" placeholder="Password"><br>
					<input class="logButton" type="submit" value="Login"> <input class="logButton" id="regBtn" type="button" value="Register"><br>
					<input class="logButton" id="forPass" type="button" value="Forgot Password"><br>
					
					<img src="/img/MUNLOGO.gif" alt="Provehito in Altum" title="Provehito in Altum">
				</form>
				<form id="regFields" name="regForm" method="post">
					<input class="regField" type="text" name="munmail" placeholder="MUN E-mail"><br>
					<input class="regName" type="text" name="gname" placeholder="Given name">
					<input class="regName" type="text" name="sname" placeholder="Surname"><br>
					<input class="regField" type="password" name="regPass" placeholder="Password"><br>
					<input class="regField" type="password" name="regPassConf" placeholder="Confirm Password"><br>
					<input type="text" class="regField" id="datePick" placeholder="Date of Birth"><br>
					<div class="regText">GENDER:</div> 
					<input type="radio" name="gender" value="male" checked> Male
					<input type="radio" name="gender" value="female"> Female
					<input type="radio" name="gender" value="other"> Other<br>
					
					<div class="spacerDiv"></div>
					
					<div class="regText"><img id="picDisp" src="/img/SEAHAWK_SIL.jpg"></div>
					<div id="profilePic"><input id="picUp" type="file" name="pic" accept="image/*"></div><br>
					
					<div class="spacerDiv"></div>
					
					<div id="tosText"><input type="checkbox" name="checkTOS">I HAVE READ AND AGREE TO THE TERMS OF SERVICE</div><br>
					
					<input class="regButton" type="button" value="Terms of Service"><br>
					<input class="regButton" type="submit" value="Register"><br>
					<input class="regButton" id="closeReg" type="button" value="Back">
				</form>
			</div>
		
		</div>
	
		<div id="rightPanel">
			<div id="postTemp">
				<span>POSTS GO HERE</span><br>
				<span>POSTS GO HERE</span><br>
				<span>POSTS GO HERE</span><br>
				<span>POSTS GO HERE</span><br>
				<span>POSTS GO HERE</span><br>
				<span>POSTS GO HERE</span><br>
				<span>POSTS GO HERE</span><br>
				<span>SHOULDN'T BE SEEN</span><br>
			</div>
			<div id="postTemp">
				<span> POSTS GO HERE </span><br>
			</div>
		</div>
	</div>
</body>



</html>